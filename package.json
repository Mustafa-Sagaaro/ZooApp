{
  "name": "zooapp",
  "homepage": "https://Mustafa-Sagaaro.github.io/ZooApp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "hosting": {
    "public": "dist", 
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ 
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "dependencies": {
    "aws-amplify": "^6.0.27",
    "axios": "^1.6.8",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "flowbite-react": "^0.7.3",
    "framer-motion": "^11.0.14",
    "pure-react-carousel": "^1.30.1",
    "react": "^18.2.0",
    "react-alice-carousel": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^0.12.1",
    "@aws-amplify/backend-cli": "^0.11.1",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "aws-cdk": "^2.136.0",
    "aws-cdk-lib": "^2.136.0",
    "constructs": "^10.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "typescript": "^5.4.4",
    "vite": "^5.1.4"
  }
}

